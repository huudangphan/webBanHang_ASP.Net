@model ShopBanHang.MultiData.MultiDataa

@{
    ViewBag.Title = "Hang";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@{ WebGrid obj1 = new WebGrid(source: Model.ctdonhang, canPage: true, rowsPerPage: 10);}@obj1.GetHtml(tableStyle: "table table-bordered table-hover",
alternatingRowStyle: "info", columns: obj1.Columns(
obj1.Column("maDH", "Mã"),
obj1.Column("maSP", "Mã sản phẩm"),
obj1.Column("soLuong", "Số lượng"),
obj1.Column("donGia", "DonGia")

));


@{ WebGrid obj = new WebGrid(source: Model.donhang, canPage: true, rowsPerPage: 10); }
@obj.GetHtml(tableStyle: "table table-bordered table-hover",
alternatingRowStyle: "info", columns: obj.Columns(

obj.Column("daThanhToan", "Thanh toán"),
obj.Column("tinhTrang", "tình trạng"),
obj.Column("ngayDat", "Ngày đặt"),
obj.Column("ngayGiao", "Ngày giao"),
obj.Column("maKH", "Mã KH")

));

