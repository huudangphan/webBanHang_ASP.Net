@model IEnumerable<ShopBanHang.Models.HDNhapSP>

@{
    ViewBag.Title = "DonHangNhap";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

@using (Html.BeginForm())
{

    <table>
        <tr>
            <td>From date &nbsp;</td>
            <td><input name="fromDate" type="date" /> &nbsp;</td>
            <td>ToDate date &nbsp;</td>
            <td><input name="toDate" type="date" /> &nbsp;</td>
            <td> <input type="submit" name="name" value="Tìm kiếm" /></td>
        </tr>

    </table>


}
<table class="table">
    <tr>
       
        <th>
            @Html.DisplayNameFor(model => model.ngayNhap)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.maKho)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
<tr>

    <td>
        @Html.DisplayFor(modelItem => item.ngayNhap)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.maKho)
    </td>
    <td>
        <a href="@Url.Action("ChiTietDonHangNhap","DonHang" ,new { @id=item.maPhieuNhap})">Chi tiết</a>
    </td>
</tr>
    }

</table>
